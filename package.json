{
  "name": "interview-prep-platform",
  "version": "1.0.0",
  "description": "Desktop interview preparation platform for LeetCode and ML System Design",
  "main": "dist/electron/main.js",
  "author": "Your Name",
  "license": "MIT",
  "private": true,
  "scripts": {
    "postinstall": "electron-rebuild",
    "start": "npm run dev",
    "dev": "concurrently \"npm run dev:react\" \"npm run dev:electron\"",
    "dev:react": "vite",
    "dev:electron": "wait-on http://localhost:5173 && cross-env NODE_ENV=development electron .",
    "build": "npm run build:react && npm run build:electron",
    "build:react": "vite build",
    "build:electron": "tsc -p electron/tsconfig.json && mkdir -p dist/database dist/electron/templates && cp -r database/*.sql dist/database/ && cp -r electron/templates/*.py dist/electron/templates/",
    "package": "npm run build && electron-builder",
    "package:mac": "npm run build && electron-builder --mac",
    "package:win": "npm run build && electron-builder --win",
    "package:linux": "npm run build && electron-builder --linux",
    "setup": "npm install && npm run build:electron && npm run db:init",
    "db:init": "node scripts/initDatabase.js",
    "db:import": "python3 scripts/import_all_questions.py",
    "db:setup": "npm run db:init && python3 scripts/import_all_questions.py",
    "test": "vitest",
    "lint": "eslint . --ext .ts,.tsx",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.68.0",
    "@monaco-editor/react": "^4.7.0",
    "@types/react-syntax-highlighter": "^15.5.13",
    "@xyflow/react": "^12.9.3",
    "axios": "^1.13.2",
    "better-sqlite3": "^12.4.1",
    "clsx": "^2.1.1",
    "date-fns": "^2.30.0",
    "dotenv": "^17.2.3",
    "html-to-image": "^1.11.13",
    "lucide-react": "^0.553.0",
    "node-fetch": "^2.7.0",
    "openai": "^6.9.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.30.2",
    "react-syntax-highlighter": "^16.1.0",
    "recharts": "^2.15.4",
    "zustand": "^4.5.7"
  },
  "devDependencies": {
    "@electron/rebuild": "^4.0.1",
    "@types/better-sqlite3": "^7.6.11",
    "@types/node": "^22.12.0",
    "@types/node-fetch": "^2.6.13",
    "@types/react": "^18.3.18",
    "@types/react-dom": "^18.3.5",
    "@typescript-eslint/eslint-plugin": "^8.24.1",
    "@typescript-eslint/parser": "^8.24.1",
    "@vitejs/plugin-react": "^4.3.4",
    "autoprefixer": "^10.4.20",
    "concurrently": "^9.2.0",
    "cross-env": "^7.0.3",
    "electron": "^33.4.0",
    "electron-builder": "^26.1.0",
    "eslint": "^9.20.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "postcss": "^8.4.51",
    "tailwindcss": "^3.4.21",
    "typescript": "^5.7.3",
    "vite": "^6.0.13",
    "vite-plugin-electron": "^0.28.10",
    "vitest": "^2.1.9",
    "wait-on": "^8.0.3"
  },
  "build": {
    "appId": "com.interviewprep.app",
    "productName": "Interview Prep Platform",
    "directories": {
      "buildResources": "build",
      "output": "release"
    },
    "files": [
      "dist/**/*",
      "node_modules/**/*",
      "package.json"
    ],
    "mac": {
      "target": "dmg",
      "icon": "build/icon.icns"
    },
    "win": {
      "target": "nsis",
      "icon": "build/icon.ico"
    },
    "linux": {
      "target": "AppImage",
      "icon": "build/icon.png"
    }
  }
}
